{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/upload",
            "destination": "/api/index.js"
        },
        {
            "source": "/login",
            "destination": "/login.html"
        },
        {
            "source": "/(.*)",
            "destination": "/$1"
        }
    ]
}